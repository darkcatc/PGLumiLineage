# PostgreSQL配置
PG_HOST=localhost
PG_PORT=5432
PG_USER=postgres
PG_PASSWORD=your_password
PG_DATABASE=your_database

# LLM配置
OPENAI_API_KEY=your_openai_api_key
# OPENAI_API_BASE=https://api.openai.com/v1  # 如果需要自定义API基础URL

